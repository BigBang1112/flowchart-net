<div class="symbol symbol-io" style="position: absolute; left: @(Data.X)px; top: @(Data.Y)px; width: @(Data.Width)px;">
    <div class="shape symbol-hoverable" style="width: calc(100% - (@(IOSymbolData.DefaultHeight)px * tan(20deg))); height: @(IOSymbolData.DefaultHeight)px;"></div>
    <span>Input / Output</span>
    <div class="connect connect-top connect-top-io"></div>
    <div class="connect connect-bottom connect-bottom-io"></div>
</div>

@code {
    [Parameter, EditorRequired]
    public required IOSymbolData Data { get; set; }
}
